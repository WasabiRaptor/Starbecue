					},
					"tailExpandState" : {
						"expand": { "properties": { "image": "<tailImage>:<tailInterpolateSize>-<tailExpandState_state>" } },
						"eat": { "properties": { "image": "<tailImage>:<tailSize>-<tailExpandState_state>.<tailExpandState_frame>" } }
					},
					"portraitMode" : {
						"head": { "properties": {} },
						"bust" : "head"
					}
				}
			}
		}
	},
	"transformationGroups" : {
		"tailOffset": {"interpolated": false}
	},
	"sounds" : {

	},
	"particleEmitters" : {

	}
