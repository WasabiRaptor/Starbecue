{
	"animations" : ["/humanoid/any/sbqModules/naga/naga.animation"],
	"occupantAnimations" : ["/humanoid/any/sbqModules/naga/sbqOccupant.animation"],
	"bodyFullbrightParts" : [
		"tail"
	],
	"modules" : {
		"belly" : {
			"default" : "/humanoid/any/sbqModules/naga/belly.sbqModule.config"
		}
	},

	"locations" :{
		"tail" : {
			"infuseType" : "tailInfuse",
			"infuseColors" : true,
			"maxSize" : 1.5, // just allow things that are slightly off to fit in
			"struggleSizes" : [0,1],
			"interpolateSizes" : [ 0,
				0.125, 0.25, 0.375, 0.5, 0.625, 0.75, 0.875, 1
			],
			"sizeChangeAnims" : { "tailExpandState" : ["expand", true], "tailSize" : ["<tailSize>"] }
		}

	},
	"states" : {
		"default" : {
			"actions" : {

				"tailInfuse" : {
					"targetSettings" : {"infusePrefs":{"tailInfuse":{"prey":true}}},
					"settings" : {"infusePrefs":{"tailInfuse":{"pred":true}}},
					"script":"tryInfuse",
					"availableScript":"infuseAvailable",
					"location":"tail",
					"voreAction":"tailInfuseExternal",
					"finishAction":"tailInfused"
				},
				"tailInfused" : {
					"targetSettings" : {"infusePrefs":{"tailInfuse":{"prey":true}}},
					"settings" : {"infusePrefs":{"tailInfuse":{"pred":true}}},
					"script":"infused",
					"availableScript":"infuseAvailable",
					"location":"tail",
					"infuseSlots" : [
						"tail"
					]
				},
				"tailInfuseExternal" : {
					"targetSettings" : {
						"infusePrefs":{"tailInfuse":{"prey":true}}
					},
					"settings" : {
						"infusePrefs":{"tailInfuse":{"pred":true}}
					},
					"script" : "tryVore",
					"availableScript" : "voreAvailable",
					"animations" : {
						"<occupant>State" : ["tailVore",true],
						"<occupant>Location" : ["default",true]
					},
					"flags" : {"infusing" : true},
					"location" : "tail",
					"throughput" : 1.5
				}

			},
			"locations" : {
				"tail" : {
					"entryTypes" : ["oralVore"],
					"struggleActions" : {
						"any" : {
							"givePredResource" : {"food":0.5}, // just a little something so endo preds can gain it

							"pressAnimations" : {"tailState" : ["s_<s_direction>_press", false],"<occupant>State":["s_<s_direction>",false,false,true]},
							"holdAnimations" : {"tailState" : ["s_<s_direction>_hold",false,false,true]},
							"releaseAnimations" : {"tailState" : ["s_<s_direction>_release", false]},
							// minimum, maximum
							"time" : [10,15], // hold time for struggles to trigger action, checked once for every second held
							"count" : [10,30] // count of struggles to trigger action
						},
						"up" : {"indicate":"cyan", "action":"moveToLocation", "args":["belly"]},
						"down" : {},
						"front" : {},
						"back" : {}
					}
				}

			},
			"interactActions" : {
			}
		}
	},
	"availableVoreTypes" : {
	},
	"availableInfuseTypes" : {
		"tailInfuse":true
	}
