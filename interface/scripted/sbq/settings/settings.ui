{ //  -*- grammar-ext: json -*-
	"style" : "window", // window (has titlebar), panel (just a frame) etc.
	"size" : [320, 256], // *internal* size (not including chrome)
	"resizable" : true,
	"minSize" : [320, 256],

	"title" : ":settingsTitle",
	"icon" : "/humanoid/any/sbqActionIcons/oralVore.png",
	"uniqueBy" : "path",
	"scripts" : [ "settings.lua" ],
	"sourceRadius": -1,
	"dismissPrev":false,
	"children" : [
		{ "type" : "tabField", "id" : "mainTabField", "layout" : "horizontal", "tabs" : [
			{ "type" : "tab", "id" : "mainSettings", "title" : ":mainSettings", "visible" : true, "color" : "ff00ff", "contents" : [
				{"mode":"horizontal"},
				{"type" : "scrollArea", "scrollDirections" : [0,1], "children" : [
					{"type":"panel","style":"convex", "id":"mainSettingsPanel" ,"children":[
						{"mode":"vertical"}
					]}
				]},
				{ "type" : "tabField", "id" : "locationTabField", "layout" : "horizontal", "tabs" : []}

			]}
		], "bottomBar" : []}
	]
}
