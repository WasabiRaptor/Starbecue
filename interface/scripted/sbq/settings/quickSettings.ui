						{"type":"label", "text":":genderTF"}
					],
					[
						{"type":"sbqSetting","setting":"indefiniteTF", "toolTip":":indefiniteTFTip"},
						{"type":"label", "text":":indefiniteTF"}
					],
					[
						{"type":"sbqSetting","setting":"permanentTF", "toolTip":":permanentTFTip"},
						{"type":"label", "text":":permanentTF"}
					]
					// [
					// 	{"type":"sbqSetting","setting":"accidentalTF", "toolTip":":accidentalTFTip"},
					// 	{"type":"label", "text":":accidentalTF"},
					// ],
					// [
					// 	{"spacing":0},
					// 	{"type":"sbqSetting","setting":"potionTF", "toolTip":":potionTFTip"},
					// 	{"type":"sbqSetting","setting":"magicTF", "toolTip":":magicTFTip"},
					// 	{"type":"sbqSetting","setting":"scienceTF", "toolTip":":scienceTFTip"},
					// ],
					// [
					// 	{"spacing":0},
					// 	{"type":"sbqSetting","setting":"voreTF", "toolTip":":voreTFTip"},
					// 	{"type":"sbqSetting","setting":"unbirthTF", "toolTip":":unbirthTFTip"},
					// ]
				]},
				{"type" : "panel", "id":"otherPrefsPanel", "expandMode" : [2,2], "style" : "convex", "children" : [
					{"type":"label", "text":":otherPrefs","inline":true},
