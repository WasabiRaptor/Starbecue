						{"type":"sbqSetting","setting":"grabbed", "toolTip":":grabbedTip"},
						{"type":"label", "text":":grabbed"}
					],
					[
						{"type":"sbqSetting","setting":"pinned", "toolTip":":pinned"},
						{"type":"label", "text":":pinned"}
					]
				]}
